<div class="container transactionContainer">
    <div class="transactionCountBar">
        <div class="transactionViewsParent">

            <div class="transactionViews">
                <div class="transactionHeader">
                    <h3>Last Transaction</h3>
                </div>
                <div class="transactionCountValues">
                    <h1><%= @transaction_time%><span>minutes ago</span></h1>
                </div>
                <div class="viewTransactionLink">
                    <p>View Transaction
                        <span><img src="<%= static_path(@conn, "/images/viewWhite.svg") %>"></span>
                    </p>
                </div>
            </div>

            <div class="transactionViews transactionViewSimilar">
                <div class="transactionHeader">
                    <h3>Total transactions</h3>
                </div>
                <div class="transactionCountValues">
                    <h1><%= @transaction_estimated_count%></h1>
                </div>
            </div>

            <div class="transactionViews transactionViewSimilar">
                <div class="transactionHeader">
                    <h3>Vanity verified</h3>
                </div>
                <div class="transactionCountValues">
                    <h1><%= @vanity_count%></h1>
                </div>
            </div>

            <div class="transactionViews transactionViewSimilar">
                <div class="transactionHeader">
                    <h3>Work Exp verified</h3>
                </div>
                <div class="transactionCountValues">
                    <h1><%= @attestation_count%></h1>
                </div>
            </div>

        </div>  
    </div>

    <div class="blockscoutCard blockscoutCardTransactionView">

        <div class="blockscoutHeader">
            <div class="blockscoutHeaderInner">
                <h2>Transactions</h2>
                <div class="viewAllTransactionLink">
               
                    <h4> <%= link(gettext("View All Transactions"), to: transaction_path(BlockScoutWeb.Endpoint, :index)) %>
                        <span><img src="<%= static_path(@conn, "/images/viewBlue.svg") %>"></span>
                    </h4>
                    <h4>View all
                        <span><img src="<%= static_path(@conn, "/images/viewBlue.svg") %>"></span>
                    </h4>
                </div>
            </div>
        </div>

        <div class="trasactionStatusViewParent">
            <div class="trasactionStatus transactionSuccess">
                <div class="transactionStatusLable">
                    <h6>Work experience verification</h6>
                    <p>SUCCESS</p>
                </div>
                <div class="transactionStatusDesctiption">
                    <h5><span>Shivhari Shankar</span> has attested to <span>Linu LD’s</span> work experience as Sr. Product Designer in SpringRole.</h5>
                    <span>
                        <a href="#">View Details
                            <img src="<%= static_path(@conn, "/images/viewBlue.svg") %>"></span>
                        </a>
                    </span>
                </div>
            </div>
            <div class="transactionTime">
                <p>1 hr ago</p>
            </div>
        </div>

        <div class="trasactionStatusViewParent">
            <div class="trasactionStatus transactionFailed">
                <div class="transactionStatusLable">
                    <h6>Work experience verification</h6>
                    <p>FAILED</p>
                </div>
                <div class="transactionStatusDesctiption">
                    <h5><span>Shivhari Shankar</span> has attested to <span>Linu LD’s</span> work experience as Sr. Product Designer in SpringRole.</h5>
                    <span>
